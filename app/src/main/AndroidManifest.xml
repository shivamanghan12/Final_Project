<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.android.newsfeedapp"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.newsfeedapp.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.android.newsfeedapp.activity.NewsFeedActivity">
        </activity>
        <activity android:name="com.android.newsfeedapp.activity.NewYorkArticlesActivity">
        </activity>
        <activity android:name="com.android.newsfeedapp.activity.WebActivity">
        </activity>
        <activity android:name="com.android.newsfeedapp.activity.WebsterActivity">
        </activity>
    </application>

</manifest>